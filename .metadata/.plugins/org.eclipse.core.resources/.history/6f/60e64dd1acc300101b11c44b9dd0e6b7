package com.number.fegin;

import java.util.List;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestParam;

import com.insurance.ed.binding.BenefitDetails;

@FeignClient(name = "ELIGIBILITY-DETERMINATION-API", url = "http://localhost:5055/api/eligibility")
public interface Eligiability {
	
	 @GetMapping("/by-ssn")
	    List<BenefitDetails> findByHolderSSN(@RequestParam("ssn") String ssn);


}
