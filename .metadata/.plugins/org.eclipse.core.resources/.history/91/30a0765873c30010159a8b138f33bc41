spring:
  application:
    name: INSURANCE
  boot:
    admin:
      client:
        url: http://localhost:1111
        instance:
          name: ${spring.application.name}

server:
  port: 8091

management:
  server:
    port: 8091       # Must be SAME as server.port
    base-path: /actuator
  endpoints:
    web:
      exposure:
        include: '*'  # very important for Admin
  endpoint:
    health:
      show-details: always

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka/
    register-with-eureka: true
    fetch-registry: true
